properties([
    parameters([
        choice(
            name: 'env', 
            choices: ['stg","prd'], 
            description: '') ,
        choice(
            name: 'branch', 
            choices: ['main","feature'], 
            description: '') 
    ])
])

pipeline {
    agent any

    stages {
        stage('Hello') {
            steps {
                echo 'Hello '
            }
        }
    }
    post {
        always {
            script {
                manager.addShortText(env)
                manager.addShortText(branch)
            }
        }
    }
}
